#define GPIOA_EN           (1U<<2)
#define UART2_EN           (1U<<13)

#define CR1_TE             (1U<<3)
#define CR1_UE             (1U<<13)
#define SR_TXE             (1U<<7)

#define SYS_FREQ           16000000
#define APB2_CLK		   SYS_FREQ

#define UART_BAUDRATE      115200
