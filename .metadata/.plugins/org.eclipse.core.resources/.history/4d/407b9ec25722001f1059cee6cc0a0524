#include "stm32f1xx.h"

#define GPIOB_EN           (1U<<3)
#define PIN11              (1U<<11)
#define BTN_PIN             PIN11



int main(void)
{
	RCC -> APB2ENR |= GPIOC_EN;

	GPIOC->CRH |= (1U << 20);
	GPIOC->CRH |= (1U << 21);
	GPIOC->CRH &= ~(1U << 22);
	GPIOC->CRH &= ~(0U << 23);

	while(1)
	{
		GPIOC->BSRR = LED_PIN;
		for(int i=0; i<100000; i++){}

		GPIOC->BSRR = (1U<<29);
		for(int i=0; i<100000; i++){}
	}
}
